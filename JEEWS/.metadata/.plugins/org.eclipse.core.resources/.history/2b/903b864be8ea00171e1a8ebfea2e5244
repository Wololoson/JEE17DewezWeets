package be.gestionhopital.Models;

public class Directeur extends Personne {
	// Variable d'instance
	private int code;

	// Constructeurs
	public Directeur() {
		super();
	}
	
	public Directeur(int code,int idPersonne,String nom,String prenom,Date dateNaiss, String numTelephone, String motDePasse) {
		super(idPersonne,nom,prenom,dateNaiss,numTelephone,motDePasse);
		this.code = code;
	}

	// Propriétés
	public int getCode() {
		return code;
	}

	public void setCode(int code) {
		this.code = code;
	}

	// Méthodes
	@Override
	public void modifierInfos() {

	};
}
